{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "cn.y",
  "version": "1.0",
  "author": "yyy",
  "createTime": "2023-11-22",
  "fileConfig": {
    "inputRootPath": ".source/acm-template",
    "outputRootPath": "D:/code/y-CodeGenerator/y-codegenerator-maker/generated/acm-template-pro-generator/generated",
    "sourceRootPath": "D:/code/y-CodeGenerator/y-generator-demo-project/acm-template",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/cn/y/acm/MainTemplate.java.ftl",
        "outputPath": "src/cn/y/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic",
        "condition": "needGit"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static",
        "condition": "needGit"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "needGit",
        "type": "boolean",
        "description": "是否生成 .gitignore 文件",
        "defaultValue": true,
        "abbr": "g"
      },
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "yyy",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}